{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "sourceMap": false,
    "esModuleInterop": true,  
    "baseUrl":"src",
    "paths" : {
      "@controllers/*": ["controllers/*"],
      "@middlewares/*": ["middlewares/*"],
      "@models/*": ["models/*"],
      "@routes/*": ["routes/*"],
      "@services/*": ["services/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["./src/**/*.ts"],

}
